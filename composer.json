{
  "name": "drupalcz/csgov",
  "type": "drupal-profile",
  "description": "A feature-rich Drupal distribution for czech and slovak eGovernment sites.",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.2",
    "bower-asset/chosen": "1.8.7",
    "bower-asset/dropzone": "5.5.0",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal/admin_toolbar": "^2.0",
    "drupal/chosen": "2.9.0",
    "drupal/components": "^1.1.0",
    "drupal/config_filter": "1.5",
    "drupal/config_ignore": "2.2",
    "drupal/config_split": "1.4",
    "drupal/config_update": "1.7",
    "drupal/console": "^1.0.2",
    "drupal/context": "4.0-beta2",
    "drupal/core": "^8.8.0",
    "drupal/core-composer-scaffold": "^9.1",
    "drupal/ctools": "^3.2.0",
    "drupal/dropzonejs": "2.1.0",
    "drupal/entity_class_formatter": "1.3.0",
    "drupal/entity_embed": "1.1",
    "drupal/entity_reference_display": "1.3.0",
    "drupal/entity_reference_revisions": "1.8",
    "drupal/environment_indicator": "^3.7",
    "drupal/facets": "^1.4",
    "drupal/field_group": "3.0",
    "drupal/focal_point": "1.4",
    "drupal/google_analytics": "3.0",
    "drupal/honeypot": "2.0",
    "drupal/inline_entity_form": "1.0-rc3",
    "drupal/linked_field": "1.3.0",
    "drupal/linkit": "5.0-beta10",
    "drupal/login_security": "1.5",
    "drupal/menu_block": "1.5",
    "drupal/menu_trail_by_path": "1.2",
    "drupal/metatag": "1.13",
    "drupal/modifiers": "1.4",
    "drupal/modifiers_pack": "1.4",
    "drupal/module_filter": "3.1",
    "drupal/paragraphs": "1.11",
    "drupal/password_policy": "dev-3.x",
    "drupal/pathauto": "1.8",
    "drupal/permissions_by_term": "2.20",
    "drupal/recaptcha": "2.5",
    "drupal/redirect": "1.5",
    "drupal/robotstxt": "1.4",
    "drupal/search_api": "1.16",
    "drupal/search_api_solr": "3.9",
    "drupal/seckit": "1.2",
    "drupal/shield": "1.4",
    "drupal/simple_sitemap": "3.6",
    "drupal/token": "1.7",
    "drupal/username_enumeration_prevention": "1.0",
    "drupal/video_embed_field": "2.4",
    "drupal/webform": "5.11",
    "drupalcz/csgov_theme": "dev-8.x-1.x",
    "drush/drush": "^9.0.0",
    "nk-o/jarallax": "^1.0",
    "oomphinc/composer-installers-extender": "^2.0",
    "vlucas/phpdotenv": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "zaporylie/composer-drupal-optimizations": "^1.0"
  },
  "require-dev": {
    "drupal/console": "^1.3",
    "drupal/devel": "^2.1"
  },
  "config": {
    "preferred-install": {
      "drupal/core": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "patches": {
    },
    "drupal-scaffold": {
      "allowed-packages": [
        "drupal/core"
      ],
      "locations": {
        "web-root": "./web"
      }
    }
  },
  "repositories": [ 
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "nk-o/jarallax",
        "version": "1.10.7",
        "dist": {
          "type": "zip",
          "url": "https://github.com/nk-o/jarallax/archive/v1.10.7.zip"
        },
        "require": {
          "composer/installers": "~1.0"
        },
        "type": "drupal-library"
      }
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
